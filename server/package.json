{
  "name": "lukkari-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "dev": "nodemon",
    "build": "tsc",
    "build:start": "tsc && node dist/server.js",
    "server": "node dist/server.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "keywords": [],
  "author": "Jesse Batman",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^12.0.0",
    "nodemon": "^1.19.0"
  }
}
